+int aw9201_ts_setup_eint(void)
+{
+	int ret = 0;
+	pinctrl_select_state(pinctrl1, eint_as_int);
+	ret = request_irq(aw9201_ts_setup.irq,
+				(irq_handler_t)aw9201_ts_eint_func,
+				IRQF_TRIGGER_LOW,
+				"aw9201-eint", NULL);
+	if (ret > 0) {
+		ret = -1;
+	}
+
+	return ret;
+}
