+static void fts_check_gesture(int gesture_id)
+{
+	int key_gesture = 0;
+	
+	int flag =0 ;
+	
+	printk("[FTS]fts_check_gesture start gesture_id==0x%x\n ",gesture_id);
+    wake_lock_timeout(&gesture_chrg_lock,TS_WAKE_LOCK_TIMEOUT);
+	
+	if(gesture_onoff_fts==0)
+	{
+		gesture_id = 0x00 ;
+		printk("[FTS]gesture_id==0x%x\n ",gesture_id);
+		return;
+	}
+	if ( gesture_button == 1 ) {	
+	
+		switch(gesture_id)	
+		{
+				case GESTURE_DOUBLECLICK: 
+					printk("gesture_id TP Gesture is KEY_GESTURE_DOUBLE_TAP");
+					fts_gesture_data = DOUBLE_TAP;
+					key_gesture = KEY_F21;
+					break;
+					
+				case GESTURE_C: 
+					printk("tengdq TP Gesture is KEY_GESTURE_C");
+					fts_gesture_data = 0xC1;
+					key_gesture = KEY_F13;
+					break;
+					
+				case GESTURE_M: 
+					printk("tengdq TP Gesture is KEY_GESTURE_M");
+					fts_gesture_data = 0xC3;
+					key_gesture = KEY_F17;
+					break;
+					
+				case GESTURE_W: 
+					printk("tengdq TP Gesture is KEY_GESTURE_W");
+					fts_gesture_data = 0xC2;
+					key_gesture = KEY_F15;
+					break;
+				
+				case GESTURE_O: 
+					printk("tengdq TP Gesture is KEY_GESTURE_O");
+					fts_gesture_data = 0xC4;
+					key_gesture = KEY_F16;
+					break;	
+				
+				case KEY_GESTURE_RIGHT: 
+					printk("tengdq TP Gesture is KEY_GESTURE_RIGHT");
+					fts_gesture_data = 0xB1;
+					key_gesture = KEY_F23;
+					break;	
+				
+				case KEY_GESTURE_LEFT: 
+					printk("tengdq TP Gesture is KEY_GESTURE_LEFT");
+					fts_gesture_data = 0xB0;
+					key_gesture = KEY_F22;
+					break;	
+				
+				default:
+						flag = 1;
+						break;
+		}
+		if(flag == 0)
+		{
+			input_report_key(tpd->dev, key_gesture, 1);
