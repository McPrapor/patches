+			dev_err(&client->dev, "[FTS] upgrade failed. err=%d.\n", i_ret);
+		}
+		else
+		{
+			#ifdef AUTO_CLB
+				fts_ctpm_auto_clb(client);  /*start auto CLB*/
+			#endif
