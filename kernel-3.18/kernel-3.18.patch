+	}
+	else if ((fts_updateinfo_curr.CHIP_ID==0x59))
+	{
+	    	if (fw_len < 8 || fw_len > 54*1024)
+		{
+		    	pr_err("FW length error\n");
+	    		return -EIO;
+	    	}
+
+	    	/*FW upgrade*/
+    		pbt_buf = CTPM_FW;
+	   	 /*call the upgrade function*/
+    		i_ret = fts_5x26_ctpm_fw_upgrade(client, pbt_buf, sizeof(CTPM_FW));
