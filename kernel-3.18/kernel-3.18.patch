+		{
+			msleep(300);
+			fts_read_reg(client, FTS_REG_FW_VER, &uc_tp_fm_ver);
+			ft_kick_wdt();
+			dev_dbg(&client->dev, "[FTS_D] upgrade to new version 0x%x\n",uc_tp_fm_ver);
