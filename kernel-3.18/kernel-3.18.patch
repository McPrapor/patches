+#define IS_SPACE(CH) (((CH) == ' ') || ((CH) == '\n'))
+/*----------------------------------------------------------------------------*/
+static int read_int_from_buf(struct stk3x1x_priv *obj, const char *buf, size_t count,
+			     u32 data[], int len)
+{
+	int idx = 0;
+	char *cur = (char *)buf, *end = (char *)(buf + count);
+
+	while (idx < len) {
+		while ((cur < end) && IS_SPACE(*cur)) {
+			cur++;
+		}
+
+		if (1 != sscanf(cur, "%d", &data[idx])) {
+			break;
+		}
+
+		idx++;
+		while ((cur < end) && !IS_SPACE(*cur)) {
+			cur++;
+		}
+	}
+	return idx;
+}
