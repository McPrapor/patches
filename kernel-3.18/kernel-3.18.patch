+/* Set the Gain range */
+int qmcX983_set_range(short range)
+{
+	int err = 0;
+	unsigned char data[2];
+	struct qmcX983_i2c_data *obj = i2c_get_clientdata(this_client);
+
+	int ran ;
+	switch (range) {
+	case QMCX983_RNG_2G:
+		ran = RNG_2G;
+		break;
+	case QMCX983_RNG_8G:
+		ran = RNG_8G;
+		break;
+	case QMCX983_RNG_12G:
+		ran = RNG_12G;
+		break;
+	case QMCX983_RNG_20G:
+		ran = RNG_20G;
+		break;
+	default:
+		return -EINVAL;
+	}
+
+	obj->xy_sensitivity = 20000/ran;
+	obj->z_sensitivity = 20000/ran;
+
+	data[0] = CTL_REG_ONE;
+	err = I2C_RxData(data, 1);
+
+	data[0] &= 0xcf;
+	data[0] |= (range << 4);
+	data[1] = data[0];
+
+	data[0] = CTL_REG_ONE;
+	err = I2C_TxData(data, 2);
+	return err;
+
+}
+
+/* Set the sensor mode */
+int qmcX983_set_mode(char mode)
+{
+	int err = 0;
+
+	unsigned char data[2];
+	data[0] = CTL_REG_ONE;
+	err = I2C_RxData(data, 1);
+
+	data[0] &= 0xfc;
+	data[0] |= mode;
+	data[1] = data[0];
+
+	data[0] = CTL_REG_ONE;
+	MSE_LOG("QMCX983 in qmcX983_set_mode, data[1] = [%02x]", data[1]);
+	err = I2C_TxData(data, 2);
+
+	return err;
+}
+
+int qmcX983_set_ratio(char ratio)
+{
+	int err = 0;
+
+	unsigned char data[2];
+	data[0] = 0x0b;//RATIO_REG;
+	data[1] = ratio;
+	err = I2C_TxData(data, 2);
+	return err;
+}
