+void fts_update_fw_vendor_id(struct fts_ts_data *data)
+{
+	struct i2c_client *client = data->client;
+	u8 reg_addr;
+	int err;
+
+	reg_addr = FTS_REG_FW_VENDOR_ID;
+	err = fts_i2c_read(client, &reg_addr, 1, &data->fw_vendor_id, 1);
