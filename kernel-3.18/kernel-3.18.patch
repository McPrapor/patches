+static int lis3dh_readChipInfo(struct i2c_client *client,
+		char *buf, int bufsize)
+{
+	u8 databuf[10];
+
+	memset(databuf, 0, sizeof(u8) * 10);
+
+	if ((NULL == buf) || (bufsize <= 30))
+		return -1;
+
+	if (NULL == client) {
+		*buf = 0;
