+/*----------------------------------------------------------------------------*/
+static int stk3x1x_delete_attr(struct device_driver *driver)
+{
+	int idx, err = 0;
+	int num = (int)(sizeof(stk3x1x_attr_list) / sizeof(stk3x1x_attr_list[0]));
+
+	if (!driver)
+		return -EINVAL;
+
+	for (idx = 0; idx < num; idx++) {
+		driver_remove_file(driver, stk3x1x_attr_list[idx]);
+	}
+
+	return err;
+}
