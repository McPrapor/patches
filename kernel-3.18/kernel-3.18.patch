+static int read_cmos_sensor(unsigned short slave_id, unsigned int addr, u8 *data)
+{
+	char pu_send_cmd[2] = {(char)(addr & 0xFF) };
+	kdSetI2CSpeed(I2C_SPEED);
+	return iReadRegI2C(pu_send_cmd, 1, data, 1, slave_id);/* 0 for good */
