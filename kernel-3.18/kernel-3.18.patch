+static int lis3dh_delete_attr(struct device_driver *driver)
+{
+	int idx, err = 0;
+	int num = (int)(sizeof(lis3dh_attr_list) / sizeof(lis3dh_attr_list[0]));
+
+	if (driver == NULL)
+		return -EINVAL;
+
+	for (idx = 0; idx < num; idx++)
+		driver_remove_file(driver, lis3dh_attr_list[idx]);
+
+	return err;
