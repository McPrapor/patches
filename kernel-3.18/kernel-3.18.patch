+static int lis3dh_create_attr(struct device_driver *driver)
+{
+	int idx, err = 0;
+	int num = (int)(sizeof(lis3dh_attr_list) / sizeof(lis3dh_attr_list[0]));
+
+	if (driver == NULL)
+		return -EINVAL;
+
+	for (idx = 0; idx < num; idx++) {
+		err = driver_create_file(driver, lis3dh_attr_list[idx]);
