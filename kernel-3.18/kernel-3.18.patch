+UINT32 S5K3L2_MIPI_RAW_SensorInit(PSENSOR_FUNCTION_STRUCT *pfFunc)
+{
+    /* To Do : Check Sensor status here */
+    if (pfFunc!=NULL)
+        *pfFunc=&sensor_func;
+    return ERROR_NONE;
+}    /*    OV5693_MIPI_RAW_SensorInit    */
