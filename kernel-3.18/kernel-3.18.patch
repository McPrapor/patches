+			MSE_ERR("MSENSOR_IOCTL_READ_FACTORY_SENSORDATA execute failed! err = %ld\n", err);
+
+		break;
+
+	/* ================================================================ */
+	default :
+		MSE_ERR("ERR: 0x%4x CMD not supported!", cmd);
+		return (-ENOIOCTLCMD);
+
+		break;
+	}
+
+	return err;
+}
