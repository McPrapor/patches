+			sensor_info->MIPIDataLowPwr2HighSpeedSettleDelayCount = imgsensor_info.pre.mipi_data_lp2hs_settle_dc;
+			break;
+	}
+	
+	return ERROR_NONE;
+}	/*	get_info  */
