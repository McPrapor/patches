+static int aw9201_ts_suspend(struct device *dev)
+{
+	if (WorkMode != 1) {
+		aw_sleepMode();
+		suspend_flag = 1;
+	}
+
+	disable_irq(aw9201_ts_setup.irq);
+	return 0;
+}
+
+static int aw9201_ts_resume(struct device *dev)
+{
+	if (WorkMode != 2) {
+		aw_normalMode();
+		suspend_flag = 0;
+		cali_flag = 1;
+		cali_num = 0;
+		cali_cnt = 0;
+	}
+
+	enable_irq(aw9201_ts_setup.irq);
+	return 0;
+}
