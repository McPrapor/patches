+{
+	int value = 0;
+	int sample_delay = 0;
+	int err;
+
+	value = (int)ns / 1000000;
+	if (value <= 5)
+		sample_delay = LIS3DH_BW_200HZ;
+	else if (value <= 10)
+		sample_delay = LIS3DH_BW_100HZ;
+	else
+		sample_delay = LIS3DH_BW_50HZ;
+
+	mutex_lock(&lis3dh_op_mutex);
