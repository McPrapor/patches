+	CAM_CALDB(" Attached!! \n");
+
+  //  i2c_register_board_info(CAM_CAL_I2C_BUSNUM, &kd_cam_cal_dev, 1);
+    if(platform_driver_register(&g_stCAM_CAL_Driver)){
+        CAM_CALERR("failed to register s5k3l2_eeprom driver\n");
